{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ES2015",
    "moduleResolution": "Node",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react",
    "paths": {
      "@emotion/core": ["./node_modules/@emotion/react"]
    }
  },
  "include":["src/**/*"],
}
